{
  "name": "MALLSETT",
  "version": "1.0.0",
  "description": "Social network for developers",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "server": "nodemon server.js  --port 8080",
    "client": "npm start --prefix client --port 8081",
    "start": "node server.js",
    "required": "concurrently nvm install 8",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "author": "Brad Traversy",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.1.0",
    "concurrently": "^3.5.1",
    "create-react-app": "^1.5.2",
    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.2.0",
    "moment": "^2.22.1",
    "mongodb": "^3.1.0-beta4",
    "mongoose": "^5.0.12",
    "nodemailer": "^4.6.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-vis": "^1.8.1",
    "react-youtube": "^7.5.0",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "nodemon": "^1.17.3"
  }
}
